<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.orechou.alfred.dao.system.PermissionMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.orechou.alfred.domain.system.Permission">
		<id column="permission_id" property="permissionId" />
		<result column="name" property="name" />
		<result column="perms" property="perms" />
		<result column="remark" property="remark" />
		<result column="create_time" property="createTime" />
		<result column="order_num" property="orderNum" />
		<result column="icon" property="icon" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        permission_id AS permissionId, name, perms, remark, create_time AS createTime, order_num AS orderNum, icon
    </sql>

</mapper>
